<?xml version="1.0" encoding="UTF-8"?>
<window>
    <defaultcontrol always="true">11</defaultcontrol>
    <allowoverlay>no</allowoverlay>
    <include>OpenClose</include>
    <controls>
        <control type="group">
            <centerleft>960</centerleft>
            <width>600</width>
            <top>10</top>
            <control type="image">
                <centerleft>300</centerleft>
                <width>510</width>
                <height>240</height>
                <texture border="8" colordiffuse="66ffffff">images/White.png</texture>
            </control>
            <control type="label" id="10" description="Heading">
                <centerleft>300</centerleft>
                <top>45</top>
                <width>390</width>
                <align>center</align>
                <font>size36</font>
                <shadowcolor>-</shadowcolor>
                <animation effect="fade" time="200">VisibleChange</animation>
            </control>
            <control type="slider" id="11" description="Slider">
                <centerleft>300</centerleft>
                <top>120</top>
                <width>390</width>
                <height>50</height>
                <onup>11</onup>
                <ondown>11</ondown>
                <onleft>11</onleft>
                <onright>11</onright>
            </control>
            <control type="label" id="12" description="Slider value">
                <centerleft>300</centerleft>
                <top>150</top>
                <width>600</width>
                <align>center</align>
                <font>size27</font>
                <shadowcolor>-</shadowcolor>
                <animation effect="fade" time="200">VisibleChange</animation>
            </control>
        </control>
    </controls>
</window>
